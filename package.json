{
  "name": "obsidian-r2-uploader",
  "version": "0.1.0",
  "description": "Upload images to Cloudflare R2 and replace with markdown links",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "npm run format && tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json",
    "format": "biome format --write .",
    "lint": "biome lint .",
    "check": "biome check .",
    "fix": "biome check --write .",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [],
  "author": "hnagato",
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "^2.2.0",
    "@types/node": "^24.3.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "aws-sdk-client-mock": "^4.0.0",
    "aws-sdk-client-mock-vitest": "^6.2.0",
    "builtin-modules": "3.3.0",
    "esbuild": "^0.25.9",
    "happy-dom": "^18.0.1",
    "jsdom": "^26.1.0",
    "obsidian": "latest",
    "tslib": "2.4.0",
    "typescript": "4.7.4",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.0.0"
  }
}
